<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:local="clr-namespace:EPVS.Views"
        x:Class="EPVS.Views.MainWindow"
        Title="悦谱转图比对系统 EPVS V2.0" Width="800" Height="600">
	<TabControl TabStripPlacement="Left">
		<TabItem Header="文件管理">
			<DockPanel>
				<!-- 上部分内容 -->
				<TextBlock Text="上部分内容" FontSize="18" HorizontalAlignment="Center" VerticalAlignment="Center" DockPanel.Dock="Top"/>

				<!-- 分割线 -->
				<Rectangle Fill="LightGray" Height="5" VerticalAlignment="Center" DockPanel.Dock="Top"/>

				<!-- 下部分内容 -->
				<TextBlock Text="Welcome to Avalonia!cc" FontSize="18" HorizontalAlignment="Center" VerticalAlignment="Center" DockPanel.Dock="Top"/>
			</DockPanel>
		</TabItem>
		<TabItem Header="转图比对">
			<!-- Tab 2 的内容 -->
		</TabItem>
	</TabControl>
</Window>



